  "license": "GPL",
  "scripts": {
    "dev": "NODE_ENV=DEV nodemon --exec ts-node ./src/app.js",
    "prebuild": "rimraf dist",
    "predeploy": "yarn build",
    "deploy": "eb deploy --profile weehan",
    "build": "webpack --config webpack.config.js",
    "lint": "eslint \"./src/**/*.*\"",
    "start": "./node_modules/pm2/bin/pm2-runtime ./dist/app.js",
    "poststart": "node ./node_modules/pm2/bin/pm2 logs",
    "start:local": "node ./dist/app",
    "test": "NODE_ENV=DEV mocha -r ts-node/register/transpile-only ./tests/*"
  },
  "devDependencies": {
    "@types/chai": "^4.2.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/sequelize": "^4.28.6",
    "@typescript-eslint/eslint-plugin": "^2.6.0",
    "@typescript-eslint/parser": "^2.6.0",
    "chai": "^4.2.0",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "pm2": "^4.1.2",
    "rimraf": "^3.0.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.21.2",
    "morgan": "^1.9.1",
    "pg": "^7.12.1",
    "sequelize": "^5.21.2"
  }
}
